                "[resourceId('Microsoft.App/managedEnvironments', variables('containerAppsEnvironmentName'))]",
                "[resourceId('Microsoft.Storage/storageAccounts/fileServices/shares', variables('storageAccountName'), 'default', 'ollama-model')]",
