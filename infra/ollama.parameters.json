{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "ollamaAppName": {
      "value": "${OLLAMA_APP_NAME}"
    },
    "userAssignedIdentityId": {
      "value": "${ACA_ENVIRONMENT_IDENTITY_ID}"
    },
    "containerAppsEnvironmentId": {
      "value": "${AZURE_CONTAINER_APPS_ENVIRONMENT_ID}"
    },
    "containerRegistryEndpoint": {
      "value": "${AZURE_CONTAINER_REGISTRY_ENDPOINT}"
    },
    "ollamaModelStorageName": {
      "value": "ollama-model-storage"
    }
  }
}